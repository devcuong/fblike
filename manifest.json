{
    "name": "FB Like",
    "version": "1.0",
    "manifest_version": 2,

    "description": "Công cụ đành cho việc like dạo",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "author": "CuongD",
    "browser_action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "Google Mail",
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*.facebook.com/*", "*://facebook.com/*"],
        "js": ["popup/js/jquery.min.js", "popup/js/content.js"]
    }]
}